msgid "Mouse &amp; Touchpad Settings"
msgstr "Ustawienia myszy i panelu dotykowego"

#: data/mouse-touchpad.appdata.xml.in:6
msgid "Configure mouse and touchpad"
msgstr "Skonfiguruj myszkę i panel dotykowy"

#: data/mouse-touchpad.appdata.xml.in:11 data/mouse-touchpad.appdata.xml.in:24
#: data/mouse-touchpad.appdata.xml.in:36
msgid "Minor updates:"
msgstr "Mniejsze aktualizacje:"

#: data/mouse-touchpad.appdata.xml.in:13
msgid "Update for OS 7"
msgstr "Aktualizacja OS 7"

#: data/mouse-touchpad.appdata.xml.in:14 data/mouse-touchpad.appdata.xml.in:26
#: data/mouse-touchpad.appdata.xml.in:41 data/mouse-touchpad.appdata.xml.in:48
#: data/mouse-touchpad.appdata.xml.in:56 data/mouse-touchpad.appdata.xml.in:69
#: data/mouse-touchpad.appdata.xml.in:77 data/mouse-touchpad.appdata.xml.in:87
msgid "Updated translations"
msgstr "Zaktualizowane tłumaczenia"

#: data/mouse-touchpad.appdata.xml.in:20 data/mouse-touchpad.appdata.xml.in:32
#: data/mouse-touchpad.appdata.xml.in:62
msgid "New features:"
msgstr "Nowe funkcje:"

#: data/mouse-touchpad.appdata.xml.in:22
msgid "New toggle for “Double-tap and move to drag”"
msgstr "Nowy przełącznik dla „Stuknij dwa razy i przesuń by przeciągnąć”"

#: data/mouse-touchpad.appdata.xml.in:34 data/mouse-touchpad.appdata.xml.in:39
msgid "Multitouch gesture options"
msgstr "Opcje gestów wielodotykowych"

#: data/mouse-touchpad.appdata.xml.in:38
msgid "Add more snap points to pointer speed sliders"
msgstr "Dodano więcej punktów przyciągania do suwaka prędkości kursora"

#: data/mouse-touchpad.appdata.xml.in:40
msgid "Redesigned touchpad settings"
msgstr "Przeprojektowano ustawienia panelu dotykowego"

#: data/mouse-touchpad.appdata.xml.in:55
msgid "Fix stretched switches when using large text"
msgstr "Naprawiono rozciągnięte przełączniki przy używaniu dużego tekstu"

#: data/mouse-touchpad.appdata.xml.in:64
msgid "Add Dwell Click and Pointer size settings"
msgstr "Dodano ustawienia długich kliknięć oraz rozmiaru kursora"

#: data/mouse-touchpad.appdata.xml.in:66
msgid "Other updates:"
msgstr "Inne aktualizacje:"

#: data/mouse-touchpad.appdata.xml.in:68
msgid "Also listen to secondary button clicks when selecting Primary Button"
msgstr ""
"Dodano nasłuchiwanie przycisku drugorzędnego przy wybieraniu przycisku "
"pierwszorzędnego"

#: data/mouse-touchpad.appdata.xml.in:76
msgid ""
"Fix an issue where primary button selection doesn't apply in left-to-right "
"languages"
msgstr ""
"Naprawiono problem z wyborem przycisku pierwszorzędnego nie działającym w "
"językach od prawej do lewej"

#: data/mouse-touchpad.appdata.xml.in:84
msgid "New Sidebar navigation"
msgstr "Nowa nawigacja przy użyciu panelu bocznego"

#: data/mouse-touchpad.appdata.xml.in:85
msgid "Fix middle click not appearing"
msgstr "Naprawiono niepojawiające się kliknięcia środkowym przyciskiem myszy"

#: data/mouse-touchpad.appdata.xml.in:86
msgid "Add Double-Click speed"
msgstr "Dodano prędkość podwójnego kliknięcia"
