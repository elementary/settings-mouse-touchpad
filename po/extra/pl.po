#: data/io.elementary.switchboard.mouse-touchpad.appdata.xml.in:5
msgid "Mouse &amp; Touchpad Settings"
msgstr "Ustawienia myszy i panelu dotykowego"

#: data/io.elementary.switchboard.mouse-touchpad.appdata.xml.in:6
msgid "Configure mouse and touchpad"
msgstr "Skonfiguruj myszkę i panel dotykowy"

#: data/io.elementary.switchboard.mouse-touchpad.appdata.xml.in:12
#: data/io.elementary.switchboard.mouse-touchpad.appdata.xml.in:24
