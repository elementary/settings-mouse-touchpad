#: data/mouse-touchpad.appdata.xml.in:6
msgid "Configure mouse and touchpad"
msgstr "Настройка мыши и сенсорной панели"

#: data/mouse-touchpad.appdata.xml.in:11 data/mouse-touchpad.appdata.xml.in:24
#: data/mouse-touchpad.appdata.xml.in:36
msgid "Minor updates:"
msgstr "Незначительные обновления:"

#: data/mouse-touchpad.appdata.xml.in:13
msgid "Update for OS 7"
msgstr "Обновлено до OS 7"

#: data/mouse-touchpad.appdata.xml.in:14 data/mouse-touchpad.appdata.xml.in:26
#: data/mouse-touchpad.appdata.xml.in:41 data/mouse-touchpad.appdata.xml.in:48
